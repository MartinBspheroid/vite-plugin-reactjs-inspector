{
  "name": "vite-plugin-reactjs-inspector",
  "version": "0.0.10",
  "packageManager": "pnpm@9.7.1",
  "description": "A Vite plugin to inspect React components",
  "type": "module",
  "keywords": [
    "reactjs",
    "vite",
    "inspector",
    "plugin"
  ],
  "license": "MIT",
  "homepage": "https://github.com/hunghg255/vite-plugin-reactjs-inspector#readme",
  "bugs": {
    "url": "https://github.com/hunghg255/vite-plugin-reactjs-inspector/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hunghg255/vite-plugin-reactjs-inspector.git"
  },
  "files": [
    "dist",
    "src/Overlay.jsx",
    "src/load.js",
    "src/styles/overlay.css"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.cjs",
      "import": "./dist/types.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "lint": "eslint src --quiet",
    "lint:fix": "eslint src --fix",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && npm publish",
    "prepublishOnly": "pnpm run build",
    "playground": "pnpm --dir ./playground run dev"
  },
  "peerDependencies": {
    "vite": ">=3"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "unplugin": "^1.12.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.25.1",
    "@babel/types": "^7.25.2",
    "@eslint-react/eslint-plugin": "^1.10.1",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^20.15.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "bumpp": "^9.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "esno": "^0.17.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "vitest": "^0.34.6"
  }
}
